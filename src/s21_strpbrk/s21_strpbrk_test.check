#include <string.h>
#include "../s21_string.h"

#test s21_strpbrk_test1
    const char *string = "Hey, yo, bitch!";
    const char *symbols = " ,!";

    ck_assert_str_eq(s21_strpbrk(string, symbols), strpbrk(string, symbols));


#test s21_strpbrk_test2
    const char *string = "!Hey, yo, bitch!";
    const char *symbols = " ,!";

    ck_assert_str_eq(s21_strpbrk(string, symbols), strpbrk(string, symbols));


